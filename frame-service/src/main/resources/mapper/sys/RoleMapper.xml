<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kylin.sys.dao.RoleDao">
	<select id="queryList" resultType="com.kylin.sys.entity.Role">
		select * from sys_role
	</select>
	<select id="save" parameterType="com.kylin.sys.entity.User">
		insert into sys_user (username) values (#{username})
	</select>
</mapper>